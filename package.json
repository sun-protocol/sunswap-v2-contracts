{
  "name": "sunswap2.0-contracts",
  "version": "1.1.0",
  "scripts": {
    "compile": "yarn truffle compile",
    "clean": "rm -rf build/",
    "develop": "yarn truffle develop",
    "test": "yarn truffle test",
    "test:clean": "yarn clean && sh truffle-test.sh",
    "migrate": "yarn tronbox migrate --network nile --f 1 --to 3",
    "eslint": "eslint --ext .js .",
    "prettier:check": "prettier --check . --config .prettierrc",
    "prettier:fix": "prettier --write . --config .prettierrc",
    "lint": "yarn eslint && yarn prettier:check",
    "coverage": "yarn truffle run coverage --network development",
    "ganache": "npx ganache-cli --allowUnlimitedContractSize -m 'grace silly cancel salmon vibrant later sentence such basket regular sunny satoshi' -e 10000000"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "axios": "^0.21.2",
    "tronbox": "git+https://github.com/suniodev02/tronbox.git#009402ebdf15bbc92cc56d6a00d40d703e3aa7ce"
  },
  "devDependencies": {
    "bn-chai": "^1.0.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ganache-cli": "6.12.2",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "solidity-coverage": "^0.7.22",
    "truffle": "5.4.10",
    "yarn": "^1.22.18"
  }
}
